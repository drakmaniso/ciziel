local foo(a: Int, b: Float) -> String =
	"{a} and {b}"

local (Int).bar(b: Float) -> String =
	"{a} and {b}"

local example-partial-application() -> Io[] =
	Io.do {
		Io.print(foo(___, 0.5).type-info)
		-- "fun(Int) -> String"

		Io.print(foo(123, ___).type-info)
		-- "fun(Float) -> String"

		Io.print(foo(___, ___).type-info)
		-- "fun(Int, Float) -> String"

		Io.print(___.bar(0.5).type-info)
		-- "fun(Int) -> String"

		Io.print(123.bar(___).type-info)
		-- "fun(Float) -> String"

		Io.print(___.bar(___).type-info)
		-- "fun(Int, Float) -> String"
	}

public main() =
	ex1 = 123.(foo(___, "abc"))
	ex2 = 123.(fun(x) -> foo(x, "abc"))
	()

